{
  "name": "@relevance/shared",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup src/**/*.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/**/*.ts --format cjs,esm --dts --clean --watch"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
